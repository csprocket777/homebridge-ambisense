{
  "pluginAlias": "Ambisense",
  "pluginType": "accessory",
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Name",
        "type": "string",
        "required": true,
        "default": "Ambisense Device"
      },
      "deviceId": {
        "title": "Device ID",
        "type": "string",
        "required": true
      },
      "deviceToken": {
        "title": "Device Token",
        "type": "string",
        "required": true
      },
      "dataPackage": {
        "title": "Data Package Name",
        "type": "string",
        "required": true
      },
      "deviceModel": {
        "title": "Device Model",
        "type": "string"
      },
      "serialNumber": {
        "title": "Serial Number",
        "type": "string",
        "required": true
      },
      "refreshInterval": {
        "title": "Ambisense API Refresh Interval (seconds)",
        "type": "number",
        "required": false,
        "default": 150
      }
    }
  }
}